public class isPrime {
public static boolean asalMi(int sayi){
    for (int i=2; i<sayi; i++){
        if (sayi % i ==0){
            return false ;
        }
    }
    return true;
}
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        for (int j=2; j<=40; j++){
            if (isPrime(j)){
                          System.out.print(j);
        System.out.println();
                            
            }
                          
                   
                }
     
            }
              
        }
